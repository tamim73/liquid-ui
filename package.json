{
  "version": "0.0.8",
  "name": "@tamim73/liquid-ui",
  "description": "Liquid UI for Solid JS",
  "license": "MIT",
  "author": "tamim73",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamim73/@tamim73/liquid-ui.git"
  },
  "homepage": "https://github.com/tamim73/@tamim73/liquid-ui#readme",
  "bugs": {
    "url": "https://github.com/tamim73/@tamim73/liquid-ui/issues"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "build": "rollup -c",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\""
  },
  "dependencies": {
    "@picocss/pico": "^1.5.5",
    "date-fns": "^2.29.3",
    "solid-js": "^1.5.1",
    "solid-styled-components": "^0.28.4"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "rollup": "^2.77.2",
    "rollup-preset-solid": "^1.4.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.5.0"
}
